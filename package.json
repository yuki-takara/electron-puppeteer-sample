{
  "name": "electron-puppeteer-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron ./",
    "build": "electron-packager ./ PuppeteerSmaple --platform=darwin --arch=x64 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yuki-takara/electron-puppeteer-sample.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yuki-takara/electron-puppeteer-sample/issues"
  },
  "homepage": "https://github.com/yuki-takara/electron-puppeteer-sample#readme",
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "puppeteer-core": "^5.2.1",
    "puppeteer-in-electron": "^3.0.3"
  },
  "build": {
    "appId": "com.electron.puppeteer11",
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "output": "dist"
    },
    "protocols": {
      "name": "myApp IM URL",
      "schemes": [
        "puppeteer-sample"
      ]
    }
  }
}
